{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["dom", "esnext"],
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["./src/**/*", "./gatsby-node.ts", "./gatsby-config.ts", "./plugins/**/*"]
}
