{
    "name": "www.panda-playgroup.co.uk",
    "private": true,
    "scripts": {
        "start": "gatsby develop",
        "build": "gatsby build",
        "ci:build": "gatsby build --prefix-paths",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "lint": "eslint .",
        "fix-lint": "eslint . --fix",
        "check-types": "npm run check-types:gatsby && npm run check-types:cypress",
        "check-types:gatsby": "tsc",
        "check-types:cypress": "tsc --project cypress/tsconfig.json",
        "test": "jest",
        "e2e": "cypress run",
        "perf": "lhci autorun",
        "check": "npm run lint && npm run check-format && npm run check-types && npm run test",
        "fc": "npm run fix-lint && npm run format && npm run check-types && npm run test",
        "bs": "npm run build && npm run serve",
        "cy": "cypress open",
        "prepare": "husky install"
    },
    "dependencies": {
        "flowbite": "^1.8.1",
        "flowbite-react": "^0.5.0",
        "gatsby": "^5.12.2",
        "gatsby-plugin-google-gtag": "^5.12.0",
        "gatsby-plugin-html-attributes": "^1.0.5",
        "gatsby-plugin-image": "^3.12.0",
        "gatsby-plugin-manifest": "^5.12.0",
        "gatsby-plugin-offline": "^6.12.0",
        "gatsby-plugin-sharp": "^5.12.0",
        "gatsby-source-contentful": "^8.12.0",
        "gatsby-transformer-sharp": "^5.12.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@lhci/cli": "^0.12.0",
        "@testing-library/cypress": "^9.0.0",
        "@testing-library/jest-dom": "^6.1.2",
        "@testing-library/react": "^14.0.0",
        "@tsconfig/cypress": "^1.0.1",
        "@types/jest": "^29.5.4",
        "@types/node": "18.17.1",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "autoprefixer": "^10.4.15",
        "babel-preset-gatsby": "^3.12.0",
        "cypress": "^12.17.4",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-cypress": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.33.2",
        "gatsby-plugin-postcss": "^6.12.0",
        "gatsby-plugin-tsconfig-paths": "^1.0.6",
        "husky": "^8.0.3",
        "jest": "^29.6.4",
        "jest-environment-jsdom": "^29.6.4",
        "jest-extended": "^4.0.1",
        "lint-staged": "^14.0.1",
        "postcss": "^8.4.28",
        "prettier": "^3.0.2",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.2.2"
    }
}
